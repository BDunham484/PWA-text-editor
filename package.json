    "license": "ISC",
    "dependencies": {
        "express": "^4.18.1",
        "if-env": "^1.0.4",
        "webpack-pwa-manifest": "^4.0.0"
    },
    "devDependencies": {
        "concurrently": "^5.3.0",
        "nodemon": "^2.0.19",
        "webpack": "^5.74.0",
        "webpack-cli": "^4.10.0"
    },
    "heroku-prebuild": "npm install --dev"
}
